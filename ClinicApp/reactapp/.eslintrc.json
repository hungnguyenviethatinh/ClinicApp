{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "settings": {
        "react": {
            "createClass": "createReactClass",
            "pragma": "React",
            "version": "detect",
            "flowVersion": "0.53"
        }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-prototype-builtins": 0,
        "no-unused-vars": 1,
        "no-useless-escape": 0,
        "react/display-name": 0,
        "react/no-children-prop": 0,
        "react/prop-types": 0
    }
}